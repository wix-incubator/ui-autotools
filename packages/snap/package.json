{
  "name": "@ui-autotools/snap",
  "version": "3.0.3",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "run-p build:cjs build:esm",
    "build:watch": "run-p build:cjs-watch build:esm-watch",
    "build:cjs": "tsc -p src --outDir cjs --module commonjs",
    "build:esm": "tsc -p src --outDir esm --module esnext",
    "build:cjs-watch": "tsc --watch -p src --outDir cjs --module commonjs",
    "build:esm-watch": "tsc --watch -p src --outDir esm --module esnext",
    "lint": "tslint -p src",
    "test": "mocha -r @ts-tools/node/r -r @stylable/node/register \"./test/**/*.spec.ts?(x)\"",
    "test:watch": "mocha -r @ts-tools/node/r -r @stylable/node/register \"./test/**/*.spec.ts?(x)\" --watch --watch-extensions ts,tsx"
  },
  "files": [
    "bin",
    "cjs",
    "esm",
    "templates"
  ],
  "engines": {
    "node": ">=8"
  },
  "repository": "git@github.com:wix-incubator/ui-autotools.git",
  "author": "Wix.com",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@ui-autotools/registry": "^4.0.0",
    "@ui-autotools/test-kit": "^2.4.1"
  },
  "bin": {
    "autotools-snap": "bin/cli.js"
  },
  "dependencies": {
    "@applitools/dom-snapshot": "^2.2.1",
    "@applitools/visual-grid-client": "^12.26.0",
    "@stylable/webpack-extensions": "^1.0.6",
    "@stylable/webpack-plugin": "^1.1.4",
    "@ui-autotools/node-utils": "^1.5.2",
    "@ui-autotools/utils": "^1.5.3",
    "chalk": "^2.4.2",
    "commander": "^3.0.0",
    "create-temp-directory": "^1.0.2",
    "glob": "^7.1.4",
    "jsdom": "^15.1.1",
    "proper-fs": "^1.1.2",
    "rimraf": "^3.0.0",
    "webpack": "^4.39.3"
  },
  "peerDependencies": {
    "@ui-autotools/registry": ">=4.0.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
