{
  "name": "schema-extrct",
  "version": "1.0.0",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "run-p build:cjs build:esm",
    "build:cjs": "tsc --project src --outDir cjs --module commonjs",
    "build:esm": "tsc --project src --outDir esm --module esnext",
    "lint": "run-p lint:src lint:test",
    "lint:src": "tslint --project src",
    "lint:test": "tslint --project test",
    "test": "mocha -r typescript-support \"test/**/*.spec.ts?(x)\" --watch-extensions ts,tsx",
    "run-browser": "webpack-dev-server --progress"
  },
  "bin": {
    "schema-extrct": "cjs/cli.js"
  },
  "files": [
    "cjs",
    "esm"
  ],
  "engines": {
    "node": ">=8"
  },
  "repository": "git@github.com:wix/ui-autotools.git",
  "author": "Wix.com",
  "license": "BSD-3-Clause",
  "dependencies": {
    "commander": "^2.15.1",
    "typescript": "~2.8.3"
  },
  "devDependencies": {
    "@types/autobahn": "^0.9.39",
    "html-webpack-externals-plugin": "^3.8.0",
    "kissfs": "^0.6.3",
    "mocha": "^5.2.0",
    "mocha-loader": "^1.1.3",
    "npm-run-all": "^4.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
