{
  "name": "a11y",
  "version": "1.0.0",
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "bin": {
    "autotools": "bin/cli.js"
  },
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "tsc --project ./ --outDir cjs --module commonjs",
    "lint": "run-p lint:src lint:test",
    "lint:src": "tslint --project src",
    "lint:test": "tslint --project test",
    "test": "mocha -r typescript-support \"test/**/*.spec.ts?(x)\" --watch-extensions ts,tsx"
  },
  "files": [
    "cjs"
  ],
  "engines": {
    "node": ">=8"
  },
  "repository": "git@github.com:wix/ui-autotools.git",
  "author": "Wix.com",
  "license": "BSD-3-Clause",
  "dependencies": {
    "sanity": "*",
    "metadata-tools": "*",
    "commander": "^2.15.1",
    "glob": "^7.1.2",
    "typescript-support": "^0.4.1"
  },
  "peerDependencies": {
    "mocha": ">=5",
    "chai": ">=4",
    "react": ">=16",
    "react-dom": ">=16"
  }
}
