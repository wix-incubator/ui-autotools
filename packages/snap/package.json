{
  "name": "@ui-autotools/snap",
  "version": "3.0.4",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "run-p build:cjs build:esm",
    "build:watch": "run-p build:cjs-watch build:esm-watch",
    "build:cjs": "tsc -p src --outDir cjs --module commonjs",
    "build:esm": "tsc -p src --outDir esm --module esnext",
    "build:cjs-watch": "yarn build:cjs -w",
    "build:esm-watch": "yarn build:esm -w",
    "test": "mocha \"./test/**/*.spec.ts?(x)\"",
    "test:watch": "yarn test -w"
  },
  "files": [
    "bin",
    "cjs",
    "esm",
    "templates"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": "git@github.com:wix-incubator/ui-autotools.git",
  "author": "Wix.com",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@ui-autotools/registry": "^4.1.0",
    "@ui-autotools/test-kit": "^2.4.2"
  },
  "bin": {
    "autotools-snap": "bin/cli.js"
  },
  "dependencies": {
    "@applitools/dom-snapshot": "^3.4.5",
    "@applitools/eyes-common": "^3.21.0",
    "@applitools/visual-grid-client": "^13.6.13",
    "@stylable/webpack-extensions": "^1.3.0",
    "@stylable/webpack-plugin": "^1.3.0",
    "@ui-autotools/node-utils": "^1.5.3",
    "@ui-autotools/utils": "^1.5.4",
    "chalk": "^4.0.0",
    "commander": "^5.0.0",
    "create-temp-directory": "^1.1.1",
    "glob": "^7.1.6",
    "jsdom": "^16.2.2",
    "rimraf": "^3.0.2",
    "webpack": "^4.42.1"
  },
  "peerDependencies": {
    "@ui-autotools/registry": ">=4.0.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
