{
  "name": "@ui-autotools/showcase",
  "version": "1.0.12",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "run-s build:cjs build:esm build:stylable",
    "build:watch": "run-p build:cjs-watch build:esm-watch",
    "build:cjs": "tsc -p src --outDir cjs --module commonjs",
    "build:esm": "tsc -p src --outDir esm --module esnext",
    "build:cjs-watch": "tsc --watch -p src --outDir cjs --module commonjs",
    "build:esm-watch": "tsc --watch -p src --outDir esm --module esnext",
    "build:stylable": "stc --srcDir src --outDir esm",
    "lint": "tslint -p src"
  },
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "engines": {
    "node": ">=8"
  },
  "repository": "git@github.com:wix-incubator/ui-autotools.git",
  "author": "Wix.com",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@ui-autotools/registry": "^2.0.1"
  },
  "dependencies": {
    "@stylable/core": "^0.1.10",
    "@stylable/webpack-plugin": "^0.1.11",
    "@ts-tools/node": "^0.6.5",
    "@ui-autotools/schema-extract": "^1.0.10",
    "@ui-autotools/utils": "^1.0.9",
    "chalk": "^2.4.1",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "koa": "^2.5.3",
    "koa-webpack": "^5.1.0",
    "ts-loader": "^5.2.1",
    "webpack": "^4.20.2"
  },
  "peerDependencies": {
    "@ui-autotools/registry": "^2.0.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
