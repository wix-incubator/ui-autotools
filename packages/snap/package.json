{
  "name": "@ui-autotools/snap",
  "version": "1.0.2",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "run-p build:cjs build:esm",
    "build:watch": "run-p build:cjs-watch build:esm-watch",
    "build:cjs": "tsc --project src --outDir cjs --module commonjs",
    "build:esm": "tsc --project src --outDir esm --module esnext",
    "build:cjs-watch": "tsc --watch --project src --outDir cjs --module commonjs",
    "build:esm-watch": "tsc --watch --project src --outDir esm --module esnext",
    "lint": "run-p lint:src lint:test",
    "lint:src": "tslint --project src",
    "lint:test": "true",
    "test": "true"
  },
  "files": [
    "cjs",
    "esm",
    "templates"
  ],
  "engines": {
    "node": ">=8"
  },
  "repository": "git@github.com:wix-incubator/ui-autotools.git",
  "author": "Wix.com",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@ui-autotools/registry": "^2.0.0"
  },
  "dependencies": {
    "@applitools/eyes.sdk.core": "^1.9.0",
    "@applitools/visual-grid-client": "^4.3.3",
    "@stylable/webpack-extensions": "^0.1.12",
    "@stylable/webpack-plugin": "^0.1.10",
    "@ui-autotools/utils": "^1.0.8",
    "chalk": "^2.4.1",
    "dotenv": "^6.0.0",
    "glob": "^7.1.3",
    "jsdom": "^12.0.0",
    "puppeteer": "^1.8.0",
    "tmp": "^0.0.33",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "@ui-autotools/registry": "^2.0.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
