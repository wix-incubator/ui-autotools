{
  "name": "@ui-autotools/utils",
  "version": "5.0.1",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "run-p build:cjs build:esm",
    "build:watch": "run-p build:cjs-watch build:esm-watch",
    "build:cjs": "tsc -p src --outDir cjs --module commonjs",
    "build:esm": "tsc -p src --outDir esm --module esnext",
    "build:cjs-watch": "yarn build:cjs -w",
    "build:esm-watch": "yarn build:esm -w"
  },
  "files": [
    "cjs",
    "esm",
    "patch-console.js"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": "git@github.com:wix-incubator/ui-autotools.git",
  "author": "Wix.com",
  "license": "BSD-3-Clause",
  "dependencies": {
    "chalk": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "koa": "^2.12.0",
    "koa-webpack": "^5.3.0",
    "puppeteer": "^3.1.0"
  },
  "peerDependencies": {
    "webpack": ">=4"
  },
  "publishConfig": {
    "access": "public"
  }
}
