{
  "name": "@ui-autotools/showcase",
  "version": "5.0.1",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "run-s build:cjs build:esm build:stylable",
    "build:watch": "run-p build:cjs-watch build:esm-watch",
    "build:cjs": "tsc -p src --outDir cjs --module commonjs",
    "build:esm": "tsc -p src --outDir esm --module esnext",
    "build:cjs-watch": "yarn build:cjs -w",
    "build:esm-watch": "yarn build:esm -w",
    "build:stylable": "stc --srcDir src --outDir esm"
  },
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": "git@github.com:wix-incubator/ui-autotools.git",
  "author": "Wix.com",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@ui-autotools/registry": "^5.0.0"
  },
  "dependencies": {
    "@stylable/core": "^3.8.1",
    "@stylable/webpack-plugin": "^3.8.1",
    "@ts-tools/node": "^2.0.0",
    "@ts-tools/webpack-loader": "^2.0.0",
    "@ui-autotools/utils": "^5.0.1",
    "@wix/typescript-schema-extract": "^1.0.3",
    "chalk": "^4.1.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.3.0",
    "koa": "^2.13.0",
    "koa-webpack": "^5.3.0",
    "webpack": "^4.44.0"
  },
  "peerDependencies": {
    "@ui-autotools/registry": ">=3.0.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
