{
  "name": "@ui-autotools/a11y",
  "version": "5.0.1",
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "run-p build:cjs build:esm",
    "build:watch": "run-p build:cjs-watch build:esm-watch",
    "build:cjs": "tsc -p src --outDir cjs --module commonjs",
    "build:esm": "tsc -p src --outDir esm --module esnext",
    "build:cjs-watch": "yarn build:cjs -w",
    "build:esm-watch": "yarn build:esm -w"
  },
  "files": [
    "bin",
    "cjs",
    "esm",
    "templates"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": "git@github.com:wix/ui-autotools.git",
  "author": "Wix.com",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@ui-autotools/registry": "^5.0.0"
  },
  "bin": {
    "autotools-a11y": "bin/cli.js"
  },
  "dependencies": {
    "@ui-autotools/node-utils": "^5.0.1",
    "@ui-autotools/utils": "^5.0.1",
    "axe-core": "^3.5.4",
    "chalk": "^4.1.0",
    "commander": "^5.1.0",
    "glob": "^7.1.6",
    "puppeteer": "^4.0.0"
  },
  "peerDependencies": {
    "@ui-autotools/registry": ">=3.0.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
