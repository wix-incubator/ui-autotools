{
  "name": "@ui-autotools/showcase",
  "version": "2.7.0",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "run-s build:cjs build:esm build:stylable",
    "build:watch": "run-p build:cjs-watch build:esm-watch",
    "build:cjs": "tsc -p src --outDir cjs --module commonjs",
    "build:esm": "tsc -p src --outDir esm --module esnext",
    "build:cjs-watch": "tsc --watch -p src --outDir cjs --module commonjs",
    "build:esm-watch": "tsc --watch -p src --outDir esm --module esnext",
    "build:stylable": "stc --srcDir src --outDir esm",
    "lint": "tslint -p src"
  },
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "engines": {
    "node": ">=8"
  },
  "repository": "git@github.com:wix-incubator/ui-autotools.git",
  "author": "Wix.com",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@ui-autotools/registry": "^3.4.0"
  },
  "dependencies": {
    "@stylable/core": "^1.0.3",
    "@stylable/webpack-plugin": "^1.1.0",
    "@ts-tools/node": "^0.9.3",
    "@ts-tools/webpack-loader": "^0.8.7",
    "@ui-autotools/utils": "^1.5.1",
    "@wix/typescript-schema-extract": "^0.1.2",
    "chalk": "^2.4.2",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "koa": "^2.7.0",
    "koa-webpack": "^5.2.1",
    "webpack": "^4.29.5"
  },
  "peerDependencies": {
    "@ui-autotools/registry": ">=3.0.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
